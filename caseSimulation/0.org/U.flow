FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../include/includeSettings"

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    giroide_outlet1
    {
        type            zeroGradient;
    }
    giroide_outlet2
    {
        type            zeroGradient;
    }
    giroide_wall
    {
        type            noSlip;
    }
    giroide_inlet1
    {
        type                flowRateInletVelocity;
		volumetricFlowRate  $flow;
		value uniform       (0 0 0);
    }
    giroide_inlet2
    {
        type                flowRateInletVelocity;
		// volumetricFlowRate  $flow;
		volumetricFlowRate  0;
		value uniform       (0 0 0);
    }
}

